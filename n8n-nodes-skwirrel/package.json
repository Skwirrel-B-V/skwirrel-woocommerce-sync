{
  "name": "n8n-nodes-skwirrel",
  "version": "0.1.0",
  "description": "n8n community node voor het Skwirrel ERP/PIM systeem — haal producten, groepen en categorieën op via de JSON-RPC API.",
  "keywords": [
    "n8n-community-node-package",
    "skwirrel",
    "erp",
    "pim",
    "products",
    "json-rpc"
  ],
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/Skwirrel-B-V/skwirrel-woocommerce-sync",
  "author": {
    "name": "Skwirrel B.V."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Skwirrel-B-V/skwirrel-woocommerce-sync.git",
    "directory": "n8n-nodes-skwirrel"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SkwirrelApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Skwirrel/Skwirrel.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
